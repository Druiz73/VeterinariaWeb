{
  "name": "express-es6-sample",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/bin/www",
    "dev": "SET NODE_ENV=development npm run watch ./src/bin/www",
    "prod": "npm run build && SET NODE_ENV=production node ./dist/bin/www",
    "build": "npm run clean && babel ./src --config-file ./.babelrc --out-dir dist",
    "clean": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json --"
  },
  "dependencies": {
    "babel-node": "0.0.1-security",
    "bcrypt": "^3.0.8",
    "bcrypt-nodejs": "0.0.3",
    "bigint-hash": "^0.2.2",
    "body-parser": "^1.20.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "ejs-mate": "^3.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "mercadopago": "^1.3.1",
    "mongoose": "^5.8.10",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rimraf": "^2.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.20.0",
    "nodemon": "^1.19.4",
    "path": "^0.12.7"
  }
}
